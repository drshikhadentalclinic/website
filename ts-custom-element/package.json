{
  "name": "vite-template-solid",
  "version": "0.0.0",
  "description": "A standard web component custom element built with SolidJS",
  "type": "module",
  "main": "dist/my-counter.umd.cjs",
  "module": "dist/my-counter.js",
  "types": "dist/types/my-counter.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "start": "vite",
    "dev": "vite",
